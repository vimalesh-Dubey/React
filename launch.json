{
        "version": "0.2.0",
        "configurations": [
            
        {
            "type": "chrome",
            "request": "launch",
            "name": "Attach to Chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        },
            {
                "type": "node",
                "request": "launch",
                "name": "Launch Program",
                "program": "${file}",
                "outFiles": [
                    "${workspaceRoot}/out/**/*.js"
                ]
            }
        ]
    }